#, c-format
msgid "Rejecting master request from client %s."
msgstr "Zamítnutí řídícího požadavku od klienta %s."

#: libs/core/src/DistributionSocketLink.cc:1811
#, c-format
msgid "Acknowledging master request from client %s."
msgstr "Schálení řídícího požadavku od klienta %s."

#: libs/core/src/DistributionSocketLink.cc:1864
#, c-format
msgid "Non-master client %s rejected master request."
msgstr "Neřídící klient %s odmítl řídící požadavek."

