#: ../libs/core/src/DistributionSocketLink.cc:121
#, c-format
msgid "Reconnecting to %s."
msgstr "Пересоединение с %s."

#. We did not succeed in starting the server. Arghh.
#: ../libs/core/src/DistributionSocketLink.cc:408
msgid "Could not enable network operation."
msgstr "Сетевые операции недоступны."

#: ../libs/core/src/DistributionSocketLink.cc:417
msgid "Disabling network operation."
msgstr "Отмена сетевых операций."

#: ../libs/core/src/DistributionSocketLink.cc:523
#: ../libs/core/src/DistributionSocketLink.cc:560
#, c-format
msgid "Connecting to %s."
msgstr "Соединение с %s."

#: ../libs/core/src/DistributionSocketLink.cc:675
#: ../libs/core/src/DistributionSocketLink.cc:706
#, c-format
msgid "Removing client %s."
msgstr "Удаление клиента %s."

#. Closing direct connection.
#: ../libs/core/src/DistributionSocketLink.cc:752
#, c-format
msgid "Disconnecting %s"
msgstr "Отсоединение от %s."

#. It's a remote client. mark it master.
#: ../libs/core/src/DistributionSocketLink.cc:915
#, c-format
msgid "Client %s is now master."
msgstr "Клиент %s теперь главный."

#. Its ME!
#: ../libs/core/src/DistributionSocketLink.cc:921
msgid "I'm now master."
msgstr "Теперь я главный."

#: ../libs/core/src/DistributionSocketLink.cc:1284
#: ../libs/core/src/DistributionSocketLink.cc:1343
#, c-format
msgid "Client %s saying hello."
msgstr "Клиент %s говорит привет."

#. Incorrect user.
#. Incorrect password.
#: ../libs/core/src/DistributionSocketLink.cc:1293
#: ../libs/core/src/DistributionSocketLink.cc:1373
#, c-format
msgid "Client %s access denied."
msgstr "Доступ клиенту %s запрещён."

#. Duplicate client. inform client that it's bogus and close.
#: ../libs/core/src/DistributionSocketLink.cc:1364
#: ../libs/core/src/DistributionSocketLink.cc:1491
#: ../libs/core/src/DistributionSocketLink.cc:1736
#, c-format
msgid "Client %s is duplicate."
msgstr "Клиент %s не уникален."

#: ../libs/core/src/DistributionSocketLink.cc:1443
#, c-format
msgid "Client %s signed off."
msgstr "Клиент %s ушёл."

#. gint port =
#: ../libs/core/src/DistributionSocketLink.cc:1527
#, c-format
msgid "Client %s is welcoming us."
msgstr "Клиент %s приглашает нас."

#: ../libs/core/src/DistributionSocketLink.cc:1769
#, c-format
msgid "Requesting master status from %s."
msgstr "Запрос состояния ведущего от %s."

#: ../libs/core/src/DistributionSocketLink.cc:1782
#, c-format
msgid "Client timeout from %s."
msgstr "Клиентский таймаут от %s."

#: ../libs/core/src/DistributionSocketLink.cc:1808
#, c-format
msgid "Rejecting master request from client %s."
msgstr "Отклонение главного запроса от клиента %s."

#: ../libs/core/src/DistributionSocketLink.cc:1813
#, c-format
msgid "Acknowledging master request from client %s."
msgstr "Подтверждение главного запроса от клиента %s."

#: ../libs/core/src/DistributionSocketLink.cc:1866
#, c-format
msgid "Non-master client %s rejected master request."
msgstr "Неглавный клиент %s отклонил главный запрос."

#: ../libs/core/src/DistributionSocketLink.cc:1870
#, c-format
msgid "Client %s rejected master request, delaying."
msgstr "Клиент %s отклонил главный запрос, задержка."

#. gint count =
#: ../libs/core/src/DistributionSocketLink.cc:1944
#, c-format
msgid "Client %s is now the new master."
msgstr "Клиент %s теперь главный."

#: ../libs/core/src/DistributionSocketLink.cc:2072
msgid "Network operation started."
msgstr "Сетевая операция начата."

#: ../libs/core/src/DistributionSocketLink.cc:2092
msgid "Accepted new client."
msgstr "Принят новый клиент."

#: ../libs/core/src/DistributionSocketLink.cc:2167
#, c-format
msgid "Client %s read error, closing."
msgstr "Ошибка чтения у клиента %s, закрытие."

#: ../libs/core/src/DistributionSocketLink.cc:2172
#: ../libs/core/src/DistributionSocketLink.cc:2239
#, c-format
msgid "Client %s closed connection."
msgstr "Клиент %s разорвал соединение."

#: ../libs/core/src/DistributionSocketLink.cc:2211
#, c-format
msgid "Client %s connected."
msgstr "Клиент %s соединился."

#: ../libs/core/src/DistributionSocketLink.cc:2244
#, c-format
msgid "Could not connect to client %s."
msgstr "Не могу соединиться с клиентом %s."

